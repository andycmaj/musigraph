{
  "presets": ["next/babel"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "lib": "./lib"
        }
      }
    ],
    [
      "styled-components",
      {
        "ssr": true,
        "displayName": true,
        "minify": false,
        "transpileTemplateLiterals": true
      }
    ],
    "@babel/proposal-export-default-from"
  ]
}
